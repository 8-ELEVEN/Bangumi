{
  "expo": {
    "description": "A React Native App for https://bgm.tv, tinygrail plugin 2.6.1",
    "version": "4.1.0",
    "android": {
      "versionCode": 4100,
      "package": "com.czy0729.bangumi",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/foreground.png",
        "backgroundColor": "#000000"
      }
    },
    "name": "Bangumi",
    "slug": "bangumi-pro",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "scheme": "bangumi",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./src/assets/images/icon.png",
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.czy0729.bangumi",
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "githubUrl": "https://github.com/czy0729/Bangumi",
    "sdkVersion": "39.0.0"
  }
}
